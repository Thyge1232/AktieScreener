{
  "1. Kvalitet (Quality Value)": {
    "description": "Filosofi: Finder fantastiske selskaber til en fair pris ved at score dem relativt til deres sektor. Fokuserer på høj, vedvarende profitabilitet og lav gæld.",
    "min_score": 70,
    "pre_filters": {
      "market_cap_filter": { "data_key": "Market Cap", "type": "binary", "operator": "gt", "value": 1000000000 }
    },
    "filters": {
      "roic_hybrid": {
        "data_key": "Return on Invested Capital", "type": "hybrid_range_scaled",
        "normalization": "sector_median_relative",
        "ranges": [
          {"min": 1.5, "max": 2.5, "base_points": 25, "scaled_points": 5},
          {"min": 1.2, "max": 1.5, "base_points": 18, "scaled_points": 7},
          {"min": 1.0, "max": 1.2, "base_points": 10, "scaled_points": 8}
        ]
      },
      "profit_margin_hybrid": {
        "data_key": "Profit Margin", "type": "hybrid_range_scaled",
        "normalization": "sector_median_relative",
        "ranges": [
          {"min": 1.5, "max": 2.5, "base_points": 12, "scaled_points": 3},
          {"min": 1.2, "max": 1.5, "base_points": 8, "scaled_points": 4},
          {"min": 1.0, "max": 1.2, "base_points": 4, "scaled_points": 4}
        ]
      },
      "debt_equity_range": {
        "data_key": "Total Debt/Equity", "type": "range",
        "normalization": "sector_median_relative_inverse",
        "ranges": [
          {"min": null, "max": 0.5, "points": 15},
          {"min": 0.5, "max": 0.8, "points": 10},
          {"min": 0.8, "max": 1.0, "points": 5},
          {"min": 1.0, "max": null, "points": 0}
        ]
      },
      "p_fcf_scaled": { "data_key": "P/Free Cash Flow", "type": "scaled", "min_value": 5, "max_value": 25, "target_min": 20, "target_max": 0 },
      "eps_5y_scaled": { "data_key": "EPS Growth Past 5 Years", "type": "scaled", "min_value": 0.05, "max_value": 0.25, "target_min": 0, "target_max": 10 },
      "pe_scaled": { "data_key": "P/E", "type": "scaled", "min_value": 5, "max_value": 25, "target_min": 10, "target_max": 0 }
    }
  },
  "2. Deep Value": {
    "description": "Filosofi: Søger 'cigar-skod'-selskaber med ekstrem rabat på deres aktiver og minimal konkursrisiko, justeret for sektorspecifik likviditet og gæld.",
    "min_score": 70,
    "pre_filters": {
      "market_cap_filter": { "data_key": "Market Cap", "type": "binary", "operator": "gt", "value": 200000000 },
      "pb_pre_filter": { "data_key": "P/B", "type": "binary", "operator": "lt", "value": 1.0 }
    },
    "filters": {
      "pb_scaled": { "data_key": "P/B", "type": "scaled", "min_value": 0.2, "max_value": 0.7, "target_min": 25, "target_max": 0 },
      "current_ratio_range": {
        "data_key": "Current Ratio", "type": "range",
        "normalization": "sector_median_relative",
        "ranges": [
          {"min": 1.5, "max": null, "points": 25},
          {"min": 1.2, "max": 1.5, "points": 18},
          {"min": 1.0, "max": 1.2, "points": 10},
          {"min": null, "max": 1.0, "points": 0}
        ]
      },
      "debt_equity_range": {
        "data_key": "Total Debt/Equity", "type": "range",
        "normalization": "sector_median_relative_inverse",
        "ranges": [
          {"min": null, "max": 0.5, "points": 20},
          {"min": 0.5, "max": 0.8, "points": 12},
          {"min": 0.8, "max": 1.0, "points": 6},
          {"min": 1.0, "max": null, "points": 0}
        ]
      },
      "ev_ebitda_scaled": { "data_key": "EV/EBITDA", "type": "scaled", "min_value": 1, "max_value": 6, "target_min": 20, "target_max": 0 },
      "cash_sh_scaled": { "data_key": "Cash/sh", "type": "scaled", "min_value": 0, "max_value": 20, "target_min": 0, "target_max": 10 }
    }
  },
  "3. Turnaround": {
    "description": "Filosofi: Identificerer kriseramte selskaber, hvor insider-aktivitet og short interest afviger markant fra markedet som helhed.",
    "min_score": 65,
    "pre_filters": {
      "market_cap_filter": { "data_key": "Market Cap", "type": "binary", "operator": "gt", "value": 200000000 },
      "perf_year_filter": { "data_key": "Performance (Year)", "type": "binary", "operator": "lt", "value": -0.20 }
    },
    "filters": {
      "sales_growth_qoq_scaled": { "data_key": "Sales Growth Quarter Over Quarter", "type": "scaled", "min_value": 0.05, "max_value": 0.30, "target_min": 0, "target_max": 30 },
      "insider_trans_range": {
        "data_key": "Insider Transactions", "type": "range",
        "boundary_type": "percentile",
        "ranges": [
          {"min": 95, "max": null, "points": 20},
          {"min": 80, "max": 95, "points": 15},
          {"min": 60, "max": 80, "points": 8},
          {"min": null, "max": 60, "points": 0}
        ]
      },
      "eps_this_year_scaled": { "data_key": "EPS Growth This Year", "type": "scaled", "min_value": 0.10, "max_value": 1.00, "target_min": 0, "target_max": 20 },
      "short_interest_range": {
        "data_key": "Short Interest", "type": "range",
        "boundary_type": "percentile",
        "ranges": [
          {"min": 95, "max": null, "points": 10},
          {"min": 85, "max": 95, "points": 7},
          {"min": 75, "max": 85, "points": 4},
          {"min": null, "max": 75, "points": 0}
        ]
      },
      "debt_equity_range": {
        "data_key": "Total Debt/Equity", "type": "range",
        "normalization": "sector_median_relative_inverse",
        "ranges": [
          {"min": null, "max": 0.8, "points": 12},
          {"min": 0.8, "max": 1.0, "points": 8},
          {"min": 1.0, "max": 1.2, "points": 4},
          {"min": 1.2, "max": null, "points": 0}
        ]
      },
      "current_ratio_scaled": { "data_key": "Current Ratio", "type": "scaled", "min_value": 1.0, "max_value": 3.0, "target_min": 0, "target_max": 8 }
    }
  },
  "4. Dividend Value": {
    "description": "Filosofi: Fokuserer på selskaber med højt udbytte og lav gæld i forhold til deres sektors normer.",
    "min_score": 75,
    "pre_filters": {
      "market_cap_filter": { "data_key": "Market Cap", "type": "binary", "operator": "gt", "value": 200000000 }
    },
    "filters": {
      "dividend_yield_range": {
        "data_key": "Dividend Yield", "type": "range",
        "normalization": "sector_median_relative",
        "ranges": [
          {"min": 2.0, "max": null, "points": 25},
          {"min": 1.5, "max": 2.0, "points": 18},
          {"min": 1.2, "max": 1.5, "points": 12},
          {"min": null, "max": 1.2, "points": 0}
        ]
      },
      "payout_ratio_scaled": { "data_key": "Payout Ratio", "type": "scaled", "normalization": "sector_median_relative_inverse", "min_value": 0.5, "max_value": 1.0, "target_min": 25, "target_max": 0 },
      "roic_scaled": { "data_key": "Return on Invested Capital", "type": "scaled", "normalization": "sector_median_relative", "min_value": 1.0, "max_value": 2.0, "target_min": 0, "target_max": 20 },
      "debt_equity_range": { "data_key": "Total Debt/Equity", "type": "range", "normalization": "sector_median_relative_inverse", "ranges": [{"min": null, "max": 0.5, "points": 15}, {"min": 0.5, "max": 1.0, "points": 8}] },
      "eps_5y_scaled": { "data_key": "EPS Growth Past 5 Years", "type": "scaled", "min_value": 0.03, "max_value": 0.20, "target_min": 0, "target_max": 10 },
      "beta_scaled": { "data_key": "Beta", "type": "scaled", "min_value": 0.2, "max_value": 0.8, "target_min": 5, "target_max": 0 }
    }
  },
  "5. GARP (Growth at a Reasonable Price)": {
    "description": "Filosofi: Finder vækstselskaber af høj kvalitet (ift. sektor) til en fornuftig pris. En hybrid mellem vækst- og værdiinvestering.",
    "min_score": 80,
    "pre_filters": {
      "market_cap_filter": { "data_key": "Market Cap", "type": "binary", "operator": "gt", "value": 200000000 },
      "peg_filter": { "data_key": "PEG", "type": "binary", "operator": "lt", "value": 1.5 }
    },
    "filters": {
      "peg_scaled": { "data_key": "PEG", "type": "scaled", "min_value": 0.5, "max_value": 1.2, "target_min": 25, "target_max": 0 },
      "eps_next_5y_scaled": { "data_key": "EPS Growth Next 5 Years", "type": "scaled", "min_value": 0.10, "max_value": 0.40, "target_min": 0, "target_max": 25 },
      "roic_hybrid": { "data_key": "Return on Invested Capital", "type": "hybrid_range_scaled", "normalization": "sector_median_relative", "ranges": [{"min": 1.2, "max": 2.0, "base_points": 15, "scaled_points": 5}, {"min": 1.0, "max": 1.2, "base_points": 8, "scaled_points": 7}] },
      "forward_pe_scaled": { "data_key": "Forward P/E", "type": "scaled", "min_value": 10, "max_value": 25, "target_min": 15, "target_max": 0 },
      "profit_margin_range": { "data_key": "Profit Margin", "type": "range", "normalization": "sector_median_relative", "ranges": [{"min": 1.2, "max": null, "points": 15}, {"min": 1.0, "max": 1.2, "points": 8}] }
    }
  },
  "6. Contrarian / Special Situations": {
    "description": "Filosofi: Satser imod markedet ved at finde selskaber, der er ekstremt upopulære (høj short-andel) men viser tegn på asymmetrisk potentiale (insider-køb).",
    "min_score": 65,
    "pre_filters": {
      "market_cap_filter": { "data_key": "Market Cap", "type": "binary", "operator": "gt", "value": 200000000 },
      "short_interest_filter": { 
        "data_key": "Short Interest", 
        "type": "binary", 
        "operator": "gt", 
        "value": 0.20 
      }
    },
    "filters": {
      "short_interest_range": {
        "data_key": "Short Interest", "type": "range", "boundary_type": "percentile", "ranges": [{"min": 98, "max": null, "points": 20}, {"min": 90, "max": 98, "points": 15}, {"min": 80, "max": 90, "points": 10}]
      },
      "insider_trans_range": {
        "data_key": "Insider Transactions", "type": "range", "boundary_type": "percentile", "ranges": [{"min": 95, "max": null, "points": 20}, {"min": 85, "max": 95, "points": 15}]
      },
      "pb_scaled": { 
        "data_key": "P/B", 
        "type": "scaled", 
        "min_value": 0.2, 
        "max_value": 3.5, 
        "target_min": 25, 
        "target_max": 0 
      },
      "ev_ebitda_scaled": { 
        "data_key": "EV/EBITDA", 
        "type": "scaled", 
        "min_value": 1, 
        "max_value": 15, 
        "target_min": 20, 
        "target_max": 0 
      },
      "perf_year_scaled": { 
        "data_key": "Performance (Year)", 
        "type": "scaled", 
        "min_value": -0.90, 
        "max_value": -0.30, 
        "target_min": 10, 
        "target_max": 0 
      },
      "analyst_recom_scaled": { "data_key": "Analyst Recom", "type": "scaled", "min_value": 3, "max_value": 5, "target_min": 0, "target_max": 5 }
    }
  },
  "7. Asset Value (Net-Net / Liquidation)": {
    "description": "Filosofi: Den reneste form for value. Finder selskaber, hvis markedsværdi er lavere end deres likviderbare aktiver minus al gæld, med stærk balance ift. sektoren.",
    "min_score": 70,
    "pre_filters": {
      "market_cap_filter": { "data_key": "Market Cap", "type": "binary", "operator": "gt", "value": 200000000 },
      "pb_filter": { "data_key": "P/B", "type": "binary", "operator": "lt", "value": 0.7 }
    },
    "filters": {
      "pb_scaled": { "data_key": "P/B", "type": "scaled", "min_value": 0.1, "max_value": 0.66, "target_min": 25, "target_max": 0 },
      "price_vs_book_scaled": { "data_key": "Price vs. Book/sh", "type": "scaled", "min_value": 0.1, "max_value": 0.66, "target_min": 15, "target_max": 0 },
      "current_ratio_range": {
        "data_key": "Current Ratio", "type": "range", "normalization": "sector_median_relative", "ranges": [{"min": 2.0, "max": null, "points": 25}, {"min": 1.5, "max": 2.0, "points": 15}]
      },
      "debt_equity_range": {
        "data_key": "Total Debt/Equity", "type": "range", "normalization": "sector_median_relative_inverse", "ranges": [{"min": null, "max": 0.2, "points": 25}, {"min": 0.2, "max": 0.5, "points": 15}]
      },
      "cash_sh_scaled": { "data_key": "Cash/sh", "type": "scaled", "min_value": 0, "max_value": 20, "target_min": 0, "target_max": 10 }
    }
  }
}