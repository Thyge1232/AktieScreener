{
  "global": {
    "cache_dir": "./multibagger_cache",
    "cache_ttl": 86400,
    "num_processes": 4,
    "profiles_to_run": ["stram", "loes", "momentum"]
  },
  "profiles": {
    "stram": {
      "strategy_type": "multibagger",
      "name": "STRAM PROFIL - Konservativ Multibagger",
      "parameters": {
        "min_market_cap": 300000000,
        "max_market_cap": 2000000000,
        "min_revenue_growth": 0.15,
        "min_eps_growth": 0.15,
        "min_gross_margin": 0.25,
        "max_debt_to_equity": 0.5,
        "max_debt_to_ebitda": 3.5,
        "min_interest_coverage": 3.5,
        "max_peg_ratio": 1.8,
        "min_us_volume": 150000,
        "min_eu_volume": 75000,
        "require_both_growth": false,
        "min_roe": 0.18
      },
      "description": "Høj kvalitet, stabile vækstaktier med stærke fundamentale nøgletal",
      "required_metrics": [
        "revenue_growth", "eps_growth", "gross_margin", 
        "debt_to_equity", "debt_to_ebitda", "interest_coverage",
        "peg_ratio", "roe"
      ]
    },
    "loes": {
      "strategy_type": "multibagger",
      "name": "LØS PROFIL - Balanceret Multibagger",
      "parameters": {
        "min_market_cap": 100000000,
        "max_market_cap": 3000000000,
        "min_revenue_growth": 0.1,
        "min_eps_growth": 0.1,
        "min_gross_margin": 0.15,
        "max_debt_to_equity": 0.8,
        "max_debt_to_ebitda": 4.0,
        "min_interest_coverage": 2.0,
        "max_peg_ratio": 2.5,
        "min_us_volume": 40000,
        "min_eu_volume": 20000,
        "require_both_growth": false,
        "min_roe": 0.1
      },
      "description": "Moderate krav med fokus på kvalitet og bæredygtig vækst",
      "required_metrics": [
        "revenue_growth", "eps_growth", "gross_margin", 
        "debt_to_equity", "debt_to_ebitda", "interest_coverage",
        "peg_ratio", "roe"
      ]
    },
    "momentum": {
      "strategy_type": "multibagger",
      "name": "MOMENTUM PROFIL - Teknisk Trend",
      "parameters": {
        "min_market_cap": 50000000,
        "max_market_cap": 5000000000,
        "min_revenue_growth": 0.1,
        "min_eps_growth": 0.1,
        "min_gross_margin": 0.1,
        "max_debt_to_equity": 1.2,
        "max_debt_to_ebitda": 5.0,
        "min_interest_coverage": 1.2,
        "max_peg_ratio": 3.0,
        "min_us_volume": 80000,
        "min_eu_volume": 40000,
        "require_both_growth": false,
        "min_roe": 0.08,
        "min_price_above_sma50": true,
        "min_rsi": 40,
        "max_rsi": 85,
        "min_volume_surge": 1.3
      },
      "description": "Teknisk momentum kombineret med realistiske fundamentale kriterier for vækst",
      "required_metrics": [
        "revenue_growth", "eps_growth", "gross_margin", 
        "debt_to_equity", "debt_to_ebitda", "interest_coverage",
        "peg_ratio", "roe", "price_above_sma50", "rsi", "volume_surge"
      ]
    }
  }
}