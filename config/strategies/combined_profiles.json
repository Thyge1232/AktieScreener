{
  "global": {
    "cache_dir": "./combined_cache",
    "cache_ttl": 86400,
    "num_processes": 4,
    "profiles_to_run": ["growth_at_reasonable_price", "quality_deep_value"]
  },
  "profiles": {
    "growth_at_reasonable_price": {
      "strategy_type": "combined",
      "name": "GARP PROFIL - Vækst til Rimelig Pris",
      "parameters": {
        "min_market_cap": 500000000,
        "max_market_cap": 10000000000,
        "min_revenue_growth": 0.1,
        "min_eps_growth": 0.1,
        "min_gross_margin": 0.2,
        "max_pe_ratio": 20.0,
        "max_pb_ratio": 3.0,
        "max_peg_ratio": 1.5,
        "max_debt_to_equity": 0.6,
        "min_roe": 0.12
      },
      "weighting": {
        "growth_score": 0.6,
        "value_score": 0.4
      },
      "description": "Kombinerer vækst og value elementer for at finde aktier med bedste risiko/afkast forhold",
      "required_metrics": [
        "revenue_growth", "eps_growth", "gross_margin", 
        "pe_ratio", "pb_ratio", "peg_ratio", "debt_to_equity", "roe"
      ]
    },
    "quality_deep_value": {
      "strategy_type": "combined",
      "name": "KVALITETS DEEP VALUE - Sikre Underprissede Aktier",
      "parameters": {
        "min_market_cap": 200000000,
        "max_market_cap": 5000000000,
        "max_pe_ratio": 10.0,
        "max_pb_ratio": 0.9,
        "min_dividend_yield": 2.0,
        "max_debt_to_equity": 0.4,
        "min_interest_coverage": 3.0,
        "min_current_ratio": 1.5,
        "min_roe": 0.1,
        "min_positive_eps_years": 8
      },
      "weighting": {
        "deep_value_score": 0.7,
        "quality_score": 0.3
      },
      "description": "Kombinerer deep value og kvalitetskriterier for at finde sikre underprissede aktier",
      "required_metrics": [
        "pe_ratio", "pb_ratio", "dividend_yield",
        "debt_to_equity", "interest_coverage", "current_ratio",
        "roe", "positive_eps_years"
      ]
    }
  }
}